{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Api",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build",
      "program": "${workspaceRoot}/src/Exceptionless.Web/bin/Debug/netcoreapp3.1/Exceptionless.Web.dll",
      "args": [],
      "cwd": "${workspaceRoot}/src/Exceptionless.Web",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "launchBrowser": {
          "enabled": true,
          "args": "${auto-detect-url}",
          "windows": {
              "command": "cmd.exe",
              "args": "/C start ${auto-detect-url}"
          },
          "osx": {
              "command": "open"
          },
          "linux": {
              "command": "xdg-open"
          }
      },
      "env": {
          "AppMode": "Development"
      }
    },
    {
      "name": "Job",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build",
      "program": "${workspaceRoot}/src/Exceptionless.Job/bin/Debug/netcoreapp3.1/Exceptionless.Job.dll",
      "args": [],
      "cwd": "${workspaceRoot}/src/Exceptionless.Job",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "env": {
          "AppMode": "Development"
      }
    }
  ]
}